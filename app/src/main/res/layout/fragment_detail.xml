<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/detailFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/open_sans"
        android:text="Detail Rumah "
        android:textSize="20sp" />

    <TextView
        android:id="@+id/titleDetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_toEndOf="@id/textview1"
        android:fontFamily="@font/open_sans"
        android:text="title"
        android:textSize="20sp"
        android:textStyle="bold"
        />
    <TextView
        android:id="@+id/textview2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textview1"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/open_sans"
        android:text="Uang Muka Rp"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/uangMuka"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_below="@id/textview1"
        android:layout_toEndOf="@id/textview2"
        android:fontFamily="@font/open_sans"
        android:text="nominal"
        android:textSize="20sp"
        android:textStyle="bold"
        />

    <ImageView
        android:id="@+id/imageViewDetail"
        android:layout_width="288pt"
        android:layout_height="80pt"
        android:layout_below="@+id/textview2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/alamat"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/imageViewDetail"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Alamat" />

    <TextView
        android:id="@+id/textViewDetailAlamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageViewDetail"
        android:layout_marginStart="4dp"
        android:layout_toEndOf="@id/alamat"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/tanah"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/alamat"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Tanah" />

    <TextView
        android:id="@+id/textViewDetailTanah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/alamat"
        android:layout_marginLeft="4dp"
        android:layout_toEndOf="@id/tanah"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/bangunan"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/tanah"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Bangunan" />

    <TextView
        android:id="@+id/textViewDetailBangunan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tanah"
        android:layout_marginLeft="4dp"
        android:layout_toEndOf="@id/bangunan"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/air"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/bangunan"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Air" />

    <TextView
        android:id="@+id/textViewDetailAir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/bangunan"
        android:layout_marginLeft="4dp"
        android:layout_toEndOf="@id/air"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/listrik"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/air"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Listrik" />

    <TextView
        android:id="@+id/textViewDetailListrik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/air"
        android:layout_marginLeft="4dp"
        android:layout_toEndOf="@id/listrik"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/kamartidur"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/listrik"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Kamar Tidur" />

    <TextView
        android:id="@+id/textViewDetailTidur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/listrik"
        android:layout_marginLeft="4dp"
        android:layout_toEndOf="@id/kamartidur"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/kamarmandi"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/kamartidur"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Kamar Mandi" />

    <TextView
        android:id="@+id/textViewDetailMandi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/kamartidur"
        android:layout_marginStart="4dp"
        android:layout_toEndOf="@id/kamarmandi"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/garasi"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/kamarmandi"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Garasi" />

    <TextView
        android:id="@+id/textViewDetailGarasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/kamarmandi"
        android:layout_marginStart="4dp"
        android:layout_toEndOf="@id/garasi"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/carport"
        android:layout_width="95dp"
        android:layout_height="20dp"
        android:layout_below="@id/garasi"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/open_sans_light"
        android:text="Carport" />

    <TextView
        android:id="@+id/textViewDetailCarport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/garasi"
        android:layout_marginStart="4dp"
        android:layout_toEndOf="@id/carport"
        android:fontFamily="@font/open_sans_bold"
        android:text="TextView" />


    <Button
        android:id="@+id/button_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/outline_button"
        android:fontFamily="@font/open_sans"
        android:text="Edit Rumah"
        android:textColor="@color/colorPrimary" />

</RelativeLayout>